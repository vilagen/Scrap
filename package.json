{
  "name": "scrap",
  "version": "1.0.0",
  "description": "Scrap Project",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:docker": "node server.js",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"node --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "David Hart",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/vilagen/Scrap"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.3.6",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.2.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^7.18.2",
    "pg-hstore": "^2.3.3",
    "react": "^16.13.1",
    "react-scripts": "^3.4.1",
    "redis": "^3.0.2",
    "sequelize": "^5.21.9"
  },
  "devDependencies": {
    "nodemon": "^2.0.4"
  }
}
